#!/bin/bash
sudo update-alternatives --remove java /usr/local/jdk/jdk1.8.0_111/jre/bin/java
sudo update-alternatives --remove keytool /usr/local/jdk/jdk1.8.0_111/jre/bin/keytool
sudo update-alternatives --remove orbd /usr/local/jdk/jdk1.8.0_111/jre/bin/orbd
sudo update-alternatives --remove pack200 /usr/local/jdk/jdk1.8.0_111/jre/bin/pack200
sudo update-alternatives --remove rmid /usr/local/jdk/jdk1.8.0_111/jre/bin/rmid
sudo update-alternatives --remove rmiregistry /usr/local/jdk/jdk1.8.0_111/jre/bin/rmiregistry
sudo update-alternatives --remove servertool /usr/local/jdk/jdk1.8.0_111/jre/bin/servertool
sudo update-alternatives --remove tnameserv /usr/local/jdk/jdk1.8.0_111/jre/bin/tnameserv
sudo update-alternatives --remove unpack200 /usr/local/jdk/jdk1.8.0_111/jre/bin/unpack200
sudo update-alternatives --remove policytool /usr/local/jdk/jdk1.8.0_111/jre/bin/policytool
sudo update-alternatives --remove jexec /usr/local/jdk/jdk1.8.0_111/jre/lib/jexec
sudo update-alternatives --remove appletviewer /usr/local/jdk/jdk1.8.0_111/bin/appletviewer
sudo update-alternatives --remove extcheck /usr/local/jdk/jdk1.8.0_111/bin/extcheck
sudo update-alternatives --remove idlj /usr/local/jdk/jdk1.8.0_111/bin/idlj
sudo update-alternatives --remove jar /usr/local/jdk/jdk1.8.0_111/bin/jar
sudo update-alternatives --remove jarsigner /usr/local/jdk/jdk1.8.0_111/bin/jarsigner
sudo update-alternatives --remove javac /usr/local/jdk/jdk1.8.0_111/bin/javac
sudo update-alternatives --remove javadoc /usr/local/jdk/jdk1.8.0_111/bin/javadoc
sudo update-alternatives --remove javah /usr/local/jdk/jdk1.8.0_111/bin/javah
sudo update-alternatives --remove javap /usr/local/jdk/jdk1.8.0_111/bin/javap
sudo update-alternatives --remove jcmd /usr/local/jdk/jdk1.8.0_111/bin/jcmd
sudo update-alternatives --remove jconsole /usr/local/jdk/jdk1.8.0_111/bin/jconsole
sudo update-alternatives --remove jdb /usr/local/jdk/jdk1.8.0_111/bin/jdb
sudo update-alternatives --remove jdeps /usr/local/jdk/jdk1.8.0_111/bin/jdeps
sudo update-alternatives --remove jhat /usr/local/jdk/jdk1.8.0_111/bin/jhat
sudo update-alternatives --remove jinfo /usr/local/jdk/jdk1.8.0_111/bin/jinfo
sudo update-alternatives --remove jmap /usr/local/jdk/jdk1.8.0_111/bin/jmap
sudo update-alternatives --remove jps /usr/local/jdk/jdk1.8.0_111/bin/jps
sudo update-alternatives --remove jrunscript /usr/local/jdk/jdk1.8.0_111/bin/jrunscript
sudo update-alternatives --remove jsadebugd /usr/local/jdk/jdk1.8.0_111/bin/jsadebugd
sudo update-alternatives --remove jstack /usr/local/jdk/jdk1.8.0_111/bin/jstack
sudo update-alternatives --remove jstat /usr/local/jdk/jdk1.8.0_111/bin/jstat
sudo update-alternatives --remove jstatd /usr/local/jdk/jdk1.8.0_111/bin/jstatd
sudo update-alternatives --remove native2ascii /usr/local/jdk/jdk1.8.0_111/bin/native2ascii
sudo update-alternatives --remove rmic /usr/local/jdk/jdk1.8.0_111/bin/rmic
sudo update-alternatives --remove schemagen /usr/local/jdk/jdk1.8.0_111/bin/schemagen
sudo update-alternatives --remove serialver /usr/local/jdk/jdk1.8.0_111/bin/serialver
sudo update-alternatives --remove wsgen /usr/local/jdk/jdk1.8.0_111/bin/wsgen
sudo update-alternatives --remove wsimport /usr/local/jdk/jdk1.8.0_111/bin/wsimport
sudo update-alternatives --remove xjc /usr/local/jdk/jdk1.8.0_111/bin/xjc
